<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.picturebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PictureBook">
        <activity android:name=".photo4"></activity>
        <activity android:name=".photo3" />
        <activity
            android:name=".photo2"
            android:label="@string/title_activity_photo3"
            android:theme="@style/Theme.PictureBook.NoActionBar" />
        <activity android:name=".photo1" />
        <activity
            android:name=".PhotoB"
            android:label="@string/title_activity_photo_b"
            android:theme="@style/Theme.PictureBook.NoActionBar" />
        <activity
            android:name=".photoA"
            android:label="@string/title_activity_photo"
            android:theme="@style/Theme.PictureBook.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>